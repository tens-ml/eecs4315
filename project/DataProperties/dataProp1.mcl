(* A student may enroll in a course once an instructor has been assigned *)

macro NEVER (R) =
[ (R) ] false
end_macro

NEVER(true* . { ENROLL(?c:String ?sid1:Nat) } . { ENROLL(?c:String ?sid2:Nat) where ((sid1) = )})


